n=int(input())


for i in range(1, n+1):
    m=int(input())
    if m<0 and m%2==1:
        print("ODD NEGATIVE")
    if m==0:
        print("NULL")
    if m>0 and m%2==1:
        print("ODD POSITIVE")
    if m<0 and m%2==0:
        print("EVEN NEGATIVE")
    if m>0 and m%2==0:
        print("EVEN POSITIVE")
